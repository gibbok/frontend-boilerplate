{
  "name": "project",
  "version": "1.0.0",
  "scripts": {
    "build-deploy": "npm run check && npm run build && npm run deploy",
    "build-deploy:all": "npm run check && npm run storybook:build && npm run build && npm run deploy:all",
    "build:start": "http-server -p 8081 ./build/project",
    "build": "webpack --mode production --output build/project/bundle.js",
    "check": "npm run tsc && npm run lint && npm run test",
    "clean": "shx rm -rf dist && shx rm -rf build && shx rm -rf coverage",
    "cy:verify": "cypress verify",
    "dep": "depcheck",
    "deploy": "gh-pages -d build/project",
    "deploy:all": "gh-pages -d build",
    "cy:ci": "npm run cy:verify && start-server-and-test build:start 8081 cy:run",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_BASE_URL=http://127.0.0.1:8081 cypress run --headless --browser chrome",
    "lint:fix": "tsc & eslint './src/**/*.{js,ts,tsx}' --fix & prettier './src/**/*.{js,ts,tsx}' --write",
    "lint": "tsc & eslint './src/**/*.{js,ts,tsx}' & prettier './src/**/*.{js,ts,tsx}' --check",
    "loki:ci": "loki --requireReference --reactUri file:./build/storybook",
    "sloc": "sloc ./ --exclude 'node_modules|dist|build|.loki|assets|coverage|/.(storyshot)'",
    "start": "webpack-dev-server --mode development --open --hot",
    "storybook:build:start": "http-server -p 6007 build/storybook",
    "storybook:build": "build-storybook -c .storybook -o build/storybook",
    "storybook": "start-storybook -p 6006",
    "test:visual:approve": "loki approve",
    "test:visual": "loki test --port 6006",
    "test:watch": "tsc && jest --watch --coverage false",
    "test": "tsc && jest",
    "tsc:watch": "tsc --watch --pretty",
    "tsc": "tsc"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cypress": "^4.9.0",
    "depcheck": "^0.9.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "http-server": "^0.12.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "loki": "^0.22.1",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "shx": "^0.3.2",
    "sloc": "^0.2.1",
    "start-server-and-test": "^1.11.0",
    "ts-jest": "^26.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
